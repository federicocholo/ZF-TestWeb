<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
   <class name="com.zf.test.TestTable" table="TestTable">
   <id name="keyIndex" column="keyIndex" type="java.lang.Integer">
